{
    "name": "lizhineng/checkpoint-tencent",
    "description": "Checkpoint provides an expressive, fluent interface to Tencent identity verification services.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Li Zhineng",
            "email": "im@zhineng.li"
        }
    ],
    "require": {
        "php": "^8.0",
        "tencentcloud/tencentcloud-sdk-php": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.16"
    },
    "autoload": {
        "psr-4": {
            "Zhineng\\Checkpoint\\Tencent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zhineng\\Checkpoint\\Tencent\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zhineng\\Checkpoint\\Tencent\\CheckpointServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
